{
  "name": "@divvy/sync",
  "version": "0.0.0",
  "main": "build/index.js",
  "scripts": {
    "build": "wrangler deploy --env production --dry-run --outdir build --node-compat true",
    "dev": "wrangler dev --local --env development",
    "deploy": "pnpm build && pnpm source-map && wrangler deploy --env production --no-bundle --node-compat false build/index.js",
    "tail-logs": "wrangler tail sync-production",
    "source-map": "sentry-cli sourcemaps inject build && sentry-cli sourcemaps upload build"
  },
  "dependencies": {
    "@divvy/cal": "workspace:^",
    "@divvy/cal-store": "workspace:^",
    "@supabase/supabase-js": "^2.21.0",
    "date-fns": "^2.30.0",
    "toucan-js": "^3.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230511.0",
    "esbuild-plugin-ignore": "^1.1.1",
    "wrangler": "^3.1.0"
  },
  "private": true
}
