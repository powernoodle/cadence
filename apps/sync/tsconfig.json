{
  "extends": "../../tsconfig.json",
  "include": ["**/*.ts"],
  "compilerOptions": {
    "outDir": "build",
    "baseUrl": "src",
    "rootDir": "src",
    "preserveConstEnums": true,
    "noEmit": true,
    "sourceMap": false,
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types/2022-11-30"],
    "skipLibCheck": true
  }
}
