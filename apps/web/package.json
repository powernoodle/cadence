{
  "name": "web",
  "private": true,
  "license": "UNLICENSED",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "NODE_ENV=development wrangler pages dev --compatibility-date 2023-05-12 --local ./public",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "NODE_ENV=production pnpm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler",
    "tail-logs": "../../source-env.sh && wrangler pages deployment tail --project-name cadence"
  },
  "dependencies": {
    "@cadence/db": "workspace:*",
    "@cadence/emotion-server": "workspace:^",
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@mantine/core": "^6.0.11",
    "@mantine/dates": "^6.0.11",
    "@mantine/hooks": "^6.0.11",
    "@mantine/remix": "^6.0.11",
    "@mantine/styles": "^6.0.11",
    "@remix-run/cloudflare": "^1.16.1",
    "@remix-run/cloudflare-pages": "^1.16.1",
    "@remix-run/css-bundle": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@supabase/auth-helpers-remix": "^0.2.0-next.3",
    "@supabase/supabase-js": "^2.21.0",
    "dayjs": "^1.11.7",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-social-login-buttons": "^3.9.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.13"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "production",
          "^production"
        ],
        "outputs": [
          "{projectRoot}/public/build",
          "{projectRoot}/functions"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
