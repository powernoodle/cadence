{
  "name": "web",
  "private": true,
  "license": "UNLICENSED",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:wrangler": "NODE_ENV=development pnpm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "NODE_ENV=production pnpm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev --experimental-local ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.16.0",
    "@remix-run/cloudflare-pages": "^1.16.0",
    "@remix-run/css-bundle": "^1.16.0",
    "@remix-run/react": "^1.16.0",
    "@supabase/auth-helpers-remix": "^0.1.9",
    "@supabase/supabase-js": "^2.21.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "db": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.16.0",
    "@remix-run/eslint-config": "^1.16.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.13"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "production",
          "^production"
        ],
        "outputs": [
          "{projectRoot}/public/build",
          "{projectRoot}/functions"
        ],
        "dependsOn": [
          "^build"
        ]
      }
    }
  }
}
